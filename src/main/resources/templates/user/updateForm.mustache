{{>/layout/header}}
<form>

    <input id="username" class="my_auth_form_box_input" type="text" placeholder="유저네임" maxlength="20" required
        readonly />
    <input id="password" class="my_auth_form_box_input" type="password" placeholder="기존 비밀번호" maxlength="20" required />
    <input id="same-password" class="my_auth_form_box_input" type="password" placeholder="수정 비밀번호" maxlength="20"
        required />
    <input class="my_auth_form_box_input" type="email" name="email" placeholder="이메일" maxlength="60" required
        readonly />
    <button type="submit" class="my_secondary_btn">회원정보수정</button>
</form>





{{>/layout/footer}}